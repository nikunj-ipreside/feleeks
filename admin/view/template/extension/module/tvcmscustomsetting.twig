<style type="text/css">
/***********************START THEME_CONTOL*****************/
  .ttvcmstheme-control .ttvtheme-option{
      border: 2px solid #e6e6e6;
      margin: 0;
  }
  .ttvcmstheme-control{
      position: fixed;
      z-index: 100;
      top:80px;
      right: -300px;
      -webkit-transition: all 0.6s ease-in-out;
      -moz-transition: all 0.6s ease-in-out;
      -ms-transition: all 0.6s ease-in-out;
      -o-transition: all 0.6s ease-in-out;
      transition: all 0.6s ease-in-out;
  }
  .ttvcmstheme-control.open {
      right: 0;
      -webkit-transition: all 0.6s ease-in-out;
      -moz-transition: all 0.6s ease-in-out;
      -ms-transition: all 0.6s ease-in-out;
      -o-transition: all 0.6s ease-in-out;
      transition: all 0.6s ease-in-out;
  }
  .ttvcmstheme-control table{
      width: 100%;
  }
  .ttvcmstheme-control .ttvtheme-control-icon{
      width: 44px;
      height: 44px;
      background-color: #222222;
      line-height: 47px;
      text-align: center;
      float: left;
      position: absolute;
      left: -44px;
      cursor: pointer;
      box-shadow: -1px 0px 5px 0px rgba(0, 0, 0, 0.30);
      -webkit-transition: 0.3s ease-in-out;
      -o-transition: 0.3s ease-in-out;
      -ms-transition: 0.3s ease-in-out;
      -moz-transition: 0.3s ease-in-out;
      transition: 0.3s ease-in-out;
  }
  .ttvcmstheme-control open{
    display: block;
  }
  .ttvcmstheme-control .ttvtheme-control-icon:hover{
     background-color: #000;
    
  }
  .ttvcmstheme-control .ttvtheme-control-icon i{
      font-size: 24px;
      color: #FFF;
      position: relative;
      top: 0px;
      height: 45px;
      line-height: 45px;
      -webkit-animation-name: spin;
        -webkit-animation-duration: 3000ms;
        -webkit-animation-iteration-count: infinite;
        -webkit-animation-timing-function: linear;
        -moz-animation-name: spin;
        -moz-animation-duration: 3000ms;
        -moz-animation-iteration-count: infinite;
        -moz-animation-timing-function: linear;
        -ms-animation-name: spin;
        -ms-animation-duration: 3000ms;
        -ms-animation-iteration-count: infinite;
        -ms-animation-timing-function: linear;
        -o-transition: rotate(3600deg);
       -webkit-transition: 0.3s ease-in-out;
      -o-transition: 0.3s ease-in-out;
      -ms-transition: 0.3s ease-in-out;
      -moz-transition: 0.3s ease-in-out;
      transition: 0.3s ease-in-out;
  }
  .ttvcmstheme-control .ttvtheme-control-icon:hover i{
      color: #FFF;
  }

  @-moz-keyframes spin {
      from { -moz-transform: rotate(0deg); }
      to { -moz-transform: rotate(360deg); }
  }
  @-webkit-keyframes spin {
      from { -webkit-transform: rotate(0deg); }
      to { -webkit-transform: rotate(360deg); }
  }
  @keyframes spin {
      from {transform:rotate(0deg);}
      to {transform:rotate(360deg);}
  }
  .ttvcmstheme-control .ttvselect-theme select{
      padding: 0;
      width: 165px;
      font-size: 12px;
      height: 30px;
      border: 1px solid #e6e6e6;
  }
  .ttvcmstheme-control .ttvselect-theme select:focus{
      outline: none;
  }
  .ttvcmstheme-control 
      width:250px;
      height: auto;
      padding:15px 30px;
      background-color: #fafafc;
      overflow: hidden;
      border-radius: 0 7px 7px 7px;
      box-shadow: 0 14px 27px rgba(0,0,0,.1);
      border: 1px solid #e6e6e6;
      display: block;
  }
  #order-confirmation .ttvcmstheme-control table td{
      border: none;
  }


  .ttvcmstheme-control.active {
      right: 0;
  } 


      color: #222222;
  }
  .ttvselect-theme-name,
  .ttvcolor-theme-name,
  .ttvtheme-layout-name,
  .ttvtheme-animation-name,
  .ttvtheme-page-loader-name,
  .ttvtheme-left-sticky-name,
  .ttvtheme-right-sticky-name,
  .ttvtheme-menu-sticky-name,
  .ttvbgcolor-theme-name,
  .ttvtheme-background-pattern-name{
      font-size: 12px;
  }

  .ttvtheme-color-box{
      float: left;
  }
  .ttvtheme-color-box input{
      width: 165px;
  }
  .ttvall-theme-content{
      margin-bottom: 15px;
  }


  .ttvtheme-color-one{
      margin-bottom: 10px;
      overflow: hidden;
  }
  td{
      padding:4px 0;
  }
  .ttvtheme-option .btn-primary{
      background-color: #222222;
      color: #FFF;
      text-align: center;
      border: none;
      font-weight: 500;
      margin: 0;
      text-transform: uppercase;
  }
  .ttvtheme-option .btn-default{
      background-color: #888;
      color: #FFF;
      text-align: center;
      margin: 0;
      border: none;
      text-transform: uppercase;
  }
  .ttvtheme-option .toggle-handle{
      background-color: #FFF;
      padding: 0px 20px;
  }
  .ttvtheme-color-box input{
      border:none;
      height: 25px;
      padding: 0 32px;
      border: 1px solid #e6e6e6;
      font-size: 12px;
  }
  .ttvtheme-control-title-name{
      float: left;
  }
  .ttvtheme-control-reset{
      background-color:#999;
      margin-bottom: 5px;
      border: none;
      color: #FFF;
      padding: 5px 15px;
      text-transform: uppercase;
      border:1px solid #eee;
      cursor: pointer;
      -webkit-transition: 0.3s ease-in-out;
      -o-transition: 0.3s ease-in-out;
      -ms-transition: 0.3s ease-in-out;
      -moz-transition: 0.3s ease-in-out;
      transition: 0.3s ease-in-out;
      display: inline-block;
  }

  .ttvtheme-control-title-name-reset-btn{
      width: 100%;
      overflow: hidden;
      display: block;
      text-align: center;
  }
  .ttvtheme-control-title-name-reset-btn p{
      font-size: 14px;
      text-transform: capitalize;
      margin-bottom: 5px;
      color: #222222;
  }
  .ttvtheme-control-reset:focus{
      outline: none;
  }
  .ttvtheme-all-pattern-wrapper{
      width: 185px;
  }

  .ttvtheme-all-pattern{
      width: 22px;
      height: 22px;
      background-color: #FFF;
      float: left;
      margin: 0 5px 5px 0;
      cursor: pointer;
  }
  .ttvtheme-all-pattern .ttvtheme-pattern-image:hover,
  .ttvtheme-all-pattern .ttvtheme-pattern-image.active{
      border: 2px solid #909090;
  }
  .ttvtheme-all-pattern .ttvtheme-pattern-image{
      width: 22px;
      height: 22px;
      border:1px solid #eee;
  }
  .ttvcms_theme_active{
      border: 2px solid #f00 !important;
  }
  .ttvtheme-bgcolor-box input{
      width: 165px;
      border: 1px solid #e6e6e6;
      height: 25px;
      padding: 0 32px;
      font-size: 12px;
  }
  .ttvtheme-background-patten p.notice{
      font-size: 10px;
      display: block;
      float: left;
  }
  .ttvtheme-left-sticky {
      display: none;
  }
  .ttvtheme-option .toggle-off.btn {
      padding: 8px 0 8px 20px;
  }
  .ttvtheme-option .toggle-on.btn {
      padding: 8px 20px 8px 0px;
  }
  .toggle-group .toggle-on,
  .toggle-group .toggle-off {
      display: -webkit-box;
      display: -ms-flexbox;
      display: flex;
      -webkit-box-pack: center;
      -ms-flex-pack: center;
      justify-content: center;
      -webkit-box-align: center;
      -ms-flex-align: center;
      align-items: center;
  }

  @media (max-width: 767px){
          .ttvcmstheme-control{
              display: none;
          }
  }

  .ttvtheme-color-two{
      display: none !important;
  }
  .ttvtheme-control-reset:hover {
      background-color: #222222;
  }
  .ttvcmstheme-control .ttvtheme-background-patten p.notice {
      color: #222222;
      margin-bottom: 0;
  }
  .lang-rtl  .ttvtheme-color-box ,
  .lang-rtl .ttvtheme-all-pattern {
      float: right;
  }
/***********************END THEME_CONTOL*****************/
</style>
{{ column_left }}

<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      <div class="pull-right">
        <button type="submit" form="form-module" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary"><i class="fa fa-save"></i></button>
        <a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a></div>
      <h1>{{ heading_title }}</h1>
      <ul class="breadcrumb">
        {% for breadcrumb in breadcrumbs %}
        <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
        {% endfor %}
      </ul>
    </div>
  </div>
  <div class="container-fluid">
    {% if error_warning %}
    <div class="alert alert-danger alert-dismissible"><i class="fa fa-exclamation-circle"></i> {{ error_warning }}
      <button type="button" class="close" data-dismiss="alert">&times;</button>
    </div>
    {% endif %}
    {% if error_main %}
    <div class="alert alert-danger alert-dismissible"><i class="fa fa-exclamation-circle"></i> {{ error_main }}
      <button type="button" class="close" data-dismiss="alert">&times;</button>
    </div>
    {% endif %}
    <form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-module" class="form-horizontal">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title"><i class="fa fa-pencil"></i> {{ text_edit }}</h3>
        </div>
           
         <div class="form-group">
          <label class="col-sm-2 control-label" for="input-tvcmscustomsetting_status">{{ entry_status }}</label>
          <div class="col-sm-10">
             <select name="tvcmscustomsetting_status" id="input-tvcmscustomsetting_status" class="form-control">
                {% if tvcmscustomsetting_status %}
                <option value="1" selected="selected">{{ text_enabled }}</option>
                <option value="0">{{ text_disabled }}</option>
                {% else %}
                <option value="1">{{ text_enabled }}</option>
                <option value="0" selected="selected">{{ text_disabled }}</option>
                {% endif %}
              </select>
          </div>
        </div>   
        
        <div class="panel-body">
            <ul class="nav nav-tabs">
              {% if form_1 %}<li class="active"><a href="#tab-themeconfiguration" data-toggle="tab">{{ tab_confi }}</a></li>{% endif %}
              {% if form_2 %}<li><a href="#tab-offer" data-toggle="tab">{{ tab_offer }}</a></li>{% endif %}
              {% if form_3 %}<li><a href="#tab-app" data-toggle="tab">{{ tab_app }}</a></li>{% endif %}
              {% if form_4 %}<li><a href="#tab-cust" data-toggle="tab">{{ tab_cust }}</a></li>{% endif %}
              <li><a href="#tab-imagesetting" data-toggle="tab">{{ text_imagesetting }}</a></li>
            </ul>

            <div class="tab-content">
              {% if form_1 %}
                <div class="tab-pane active" id="tab-themeconfiguration">
                    <div class="form-group"> 
                        <label class="col-sm-2 control-label" for="input-status">{{ text_theme_op }}</label>                 
                        <div class="col-sm-10">
                          <label class="radio-block">
                            <input type="radio" class="themecolor" {% if tvcmscustomsetting_configuration.themeoption == "theme1" %} checked="checked" {% endif %} name="tvcmscustomsetting_configuration[themeoption]" value="theme1" /> {{ text_theme_op1 }}
                          </label><br>
                          
                          <label class="radio-block">
                            <input type="radio" class="themecolor" {% if tvcmscustomsetting_configuration.themeoption == "theme_custom" %} checked="checked" {% endif %} name="tvcmscustomsetting_configuration[themeoption]" value="theme_custom"  /> {{ text_theme_op5 }}
                          </label><br>

                          {{ text_theme_opdes }}
                        </div>
                    </div>
                   
                    <div class="form-group input-customthemecolor {% if tvcmscustomsetting_configuration.themeoption == "theme1" %} hidden {% endif %}"> 
                      <label class="col-sm-2 control-label" for="input-customthemecolor">{{ text_theme_colo }}</label>
                      <div class="col-sm-10">
                        <input type="text" id='custom_theme_color' name="tvcmscustomsetting_configuration[customthemecolor]" value="{{ tvcmscustomsetting_configuration.customthemecolor }}" placeholder="{{ text_tab_nopro }}" id="input-tvcmscustomsetting_configuration_no_pro" class="form-control" />
                      </div>
                    </div>
                   
                    <div class="form-group"> 
                      <label class="col-sm-2 control-label" for="input-boxlayout">{{ text_theme_box }}</label>
                      <div class="col-sm-10">
                        
                        <label class="radio-inline"> {% if tvcmscustomsetting_configuration.boxlayout %}
                          <input type="radio" class="themebox" name="tvcmscustomsetting_configuration[boxlayout]" value="1" checked="checked" />
                          {{ entry_yes }}
                          {% else %}
                          <input type="radio" class="themebox" name="tvcmscustomsetting_configuration[boxlayout]" value="1" />
                          {{ entry_yes }}
                          {% endif %} </label>
                        <label class="radio-inline"> {% if not tvcmscustomsetting_configuration.boxlayout %}
                          <input type="radio" class="themebox" name="tvcmscustomsetting_configuration[boxlayout]" value="0" checked="checked" />
                          {{ entry_no}}
                          {% else %}
                          <input type="radio" class="themebox" name="tvcmscustomsetting_configuration[boxlayout]" value="0" />
                          {{ entry_no }}
                          {% endif %} </label><br>
                          {{ text_theme_box_des }}
                      </div>
                    </div>
                    <div class="form-group themebox1 {% if not tvcmscustomsetting_configuration.boxlayout %} hidden {% endif %}" >
                      <label class="col-sm-2 control-label" for="input-backgroundtheme">{{ text_theme_bgcol }}</label>
                      <div class="col-sm-10">

                        
                        <label class="radio-inline">

                          <input type="radio" class="patternselect" {% if tvcmscustomsetting_configuration.backgroundtheme == "bgcolor" %} checked="checked" {% endif %} name="tvcmscustomsetting_configuration[backgroundtheme]" value="bgcolor"  />
                          {{ text_theme_bgcol_col }}</label>
                         <label class="radio-inline">
                          <input type="radio" {% if tvcmscustomsetting_configuration.backgroundtheme == "pattern" %} checked="checked" {% endif %} class="patternselect" name="tvcmscustomsetting_configuration[backgroundtheme]" value="pattern" />{{ text_theme_bgcol_patt }}</label><br>
                          
                           
                       
                          {{ text_theme_bgcol_des }}
                      </div>
                    </div>
                    <div class="form-group themebox1 pattencolor {% if not tvcmscustomsetting_configuration.boxlayout %} hidden {% endif %}">
                      <label class="col-sm-2 control-label" for="input-backgroundthemecolor">{{ text_theme_bgcol_col_col }}</label>
                      <div class="col-sm-10">
                        
                        <input type="text" id='background_theme_color'name="tvcmscustomsetting_configuration[backgroundthemecolor]" value="{{ tvcmscustomsetting_configuration.backgroundthemecolor }}" placeholder="{{ text_tab_nopro }}" id="input-tvcmscustomsetting_configuration_no_pro" class="form-control" />
                        

                      </div>
                    </div>
                    <div class="form-group themebox1 patten {% if not tvcmscustomsetting_configuration.boxlayout %} hidden {% endif %}">
                      <label class="col-sm-2 control-label" for="input-backgroundpattern">{{ text_theme_bgpatt }}</label>
                      <div class="col-sm-10">
                          <div class="ttvtheme-all-pattern-wrapper">
                           {% for i in 1..30 %}
                           
                                <div class="ttvtheme-all-pattern">
                                    <div id="pattern{{ i }}" class="{% if tvcmscustomsetting_configuration.backgroundpattern == 'pattern' ~ i %} active {% endif %}ttvtheme-pattern-image hrxtheme-pattern-image{{ i }}" style="background-image:url('{{ dir_pattern }}{{ i }}.png')"></div>
                                </div>
                            {% endfor %} 
                          </div>
                        <input type="text" name="tvcmscustomsetting_configuration[backgroundpattern]" value="{{ tvcmscustomsetting_configuration.backgroundpattern }}" placeholder="{{ text_tab_nopro }}" id="idpatternval" class="form-control hidden" />
                        {{ text_theme_bgpatt_des }}
                      </div>
                    </div>
                    <div class="form-group"> 
                      <label class="col-sm-2 control-label" for="input-themeoptionstatus">{{ text_theme_op_st }}</label>
                      <div class="col-sm-10">
                        
                        <label class="radio-inline"> 
                          
                          <input type="radio" {% if tvcmscustomsetting_configuration.themeoptionstatus == "1" %} checked="checked" {% endif %} name="tvcmscustomsetting_configuration[themeoptionstatus]" value="1" />
                          {{ entry_yes }}</label>
                           <label class="radio-inline"> 
                          <input type="radio" {% if tvcmscustomsetting_configuration.themeoptionstatus != "1" %} checked="checked" {% endif %} name="tvcmscustomsetting_configuration[themeoptionstatus]" value="0" />
                          {{ entry_no }}
                           </label>
                        <br>
                          {{ ttext_theme_op_st_des }}
                      </div>
                    </div>
                    {% if page_loader %}
                    <div class="form-group"> 
                      <label class="col-sm-2 control-label" for="input-pageloader">{{ text_theme_pa_lo }}</label>
                      <div class="col-sm-10">

                        <label class="radio-inline"> {% if tvcmscustomsetting_configuration.pageloader %}
                          <input type="radio" name="tvcmscustomsetting_configuration[pageloader]" value="1" checked="checked" />
                          {{ entry_yes }}
                          {% else %}
                          <input type="radio" name="tvcmscustomsetting_configuration[pageloader]" value="1" />
                          {{ entry_yes }}
                          {% endif %} </label>
                        <label class="radio-inline"> {% if not tvcmscustomsetting_configuration.pageloader %}
                          <input type="radio" name="tvcmscustomsetting_configuration[pageloader]" value="0" checked="checked" />
                          {{ entry_no }}
                          {% else %}
                          <input type="radio" name="tvcmscustomsetting_configuration[pageloader]" value="0" />
                          {{ entry_no }}
                          {% endif %} </label>

                          <br>
                          {{ text_theme_pa_lo_des }}
                      </div>
                    </div>
                    {% endif %}
                    {% if wow_js %}
                      <div class="form-group hidden"> 
                        <label class="col-sm-2 control-label" for="input-wowjs">{{ text_theme_wow }}</label>
                        <div class="col-sm-10">

                          <label class="radio-inline"> {% if tvcmscustomsetting_configuration.wowjs %}
                            <input type="radio" name="tvcmscustomsetting_configuration[wowjs]" value="1" checked="checked" />
                            {{ entry_yes }}
                            {% else %}
                            <input type="radio" name="tvcmscustomsetting_configuration[wowjs]" value="1" />
                            {{ entry_yes }}
                            {% endif %} </label>
                          <label class="radio-inline"> {% if not tvcmscustomsetting_configuration.wowjs %}
                            <input type="radio" name="tvcmscustomsetting_configuration[wowjs]" value="0" checked="checked" />
                            {{ entry_no }}
                            {% else %}
                            <input type="radio" name="tvcmscustomsetting_configuration[wowjs]" value="0" />
                            {{ entry_no }}
                            {% endif %} </label>
                          <br>
                            {{ text_theme_wow_des }}
                        </div>
                      </div>
                    {% endif %}
                    {% if mouse_hover_image %}
                    <div class="form-group"> 
                      <label class="col-sm-2 control-label" for="input-mousehoverimage">{{ text_theme_mouse }}</label>
                      <div class="col-sm-10">

                        <label class="radio-inline"> {% if tvcmscustomsetting_configuration.mousehoverimage %}
                          <input type="radio" name="tvcmscustomsetting_configuration[mousehoverimage]" value="1" checked="checked" />
                          {{ entry_yes }}
                          {% else %}
                          <input type="radio" name="tvcmscustomsetting_configuration[mousehoverimage]" value="1" />
                          {{ entry_yes }}
                          {% endif %} </label>
                        <label class="radio-inline"> {% if not tvcmscustomsetting_configuration.mousehoverimage %}
                          <input type="radio" name="tvcmscustomsetting_configuration[mousehoverimage]" value="0" checked="checked" />
                          {{ entry_no }}
                          {% else %}
                          <input type="radio" name="tvcmscustomsetting_configuration[mousehoverimage]" value="0" />
                          {{ entry_no }}
                          {% endif %} </label>
                        <br>
                          {{ text_theme_mouse_des }}
                      </div>
                    </div>
                    {% endif %}
                    {% if tab_product_double_row %}
                    <div class="form-group hidden"> 
                      <label class="col-sm-2 control-label" for="input-tabproductdoublerow">{{ text_theme_tap }}</label>
                      <div class="col-sm-10">

                        <label class="radio-inline"> {% if tvcmscustomsetting_configuration.tabproductdoublerow %}
                          <input type="radio" name="tvcmscustomsetting_configuration[tabproductdoublerow]" value="1" checked="checked" />
                          {{ entry_yes }}
                          {% else %}
                          <input type="radio" name="tvcmscustomsetting_configuration[tabproductdoublerow]" value="1" />
                          {{ entry_yes }}
                          {% endif %} </label>
                        <label class="radio-inline"> {% if not tvcmscustomsetting_configuration.tabproductdoublerow %}
                          <input type="radio" name="tvcmscustomsetting_configuration[tabproductdoublerow]" value="0" checked="checked" />
                          {{ entry_no }}
                          {% else %}
                          <input type="radio" name="tvcmscustomsetting_configuration[tabproductdoublerow]" value="0" />
                          {{ entry_no }}
                          {% endif %} </label>
                        <br>
                          {{ text_theme_tap_des }}
                      </div>
                    </div>
                    {% endif %}
                    {% if product_color %}
                    <div class="form-group hidden"> 
                      <label class="col-sm-2 control-label" for="input-productcolor">{{ text_theme_pro_col }}</label>
                      <div class="col-sm-10">
                        <label class="radio-inline"> {% if tvcmscustomsetting_configuration.productcolor %}
                          <input type="radio" name="tvcmscustomsetting_configuration[productcolor]" value="1" checked="checked" />
                          {{ entry_yes }}
                          {% else %}
                          <input type="radio" name="tvcmscustomsetting_configuration[productcolor]" value="1" />
                          {{ entry_yes }}
                          {% endif %} </label>
                        <label class="radio-inline"> {% if not tvcmscustomsetting_configuration.productcolor %}
                          <input type="radio" name="tvcmscustomsetting_configuration[productcolor]" value="0" checked="checked" />
                          {{ entry_no }}
                          {% else %}
                          <input type="radio" name="tvcmscustomsetting_configuration[productcolor]" value="0" />
                          {{ entry_no }}
                          {% endif %} </label>
                        <br>
                          {{ text_theme_pro_col_des }}
                      </div>
                    </div>
                    {% endif %}
                    {% if main_menu_sticky %}
                    <div class="form-group"> 
                      <label class="col-sm-2 control-label" for="input-mainmenustickystatus">{{ text_theme_main }}</label>
                      <div class="col-sm-10">

                        <label class="radio-inline"> {% if tvcmscustomsetting_configuration.mainmenustickystatus %}
                          <input type="radio" name="tvcmscustomsetting_configuration[mainmenustickystatus]" value="1" checked="checked" />
                          {{ entry_yes }}
                          {% else %}
                          <input type="radio" name="tvcmscustomsetting_configuration[mainmenustickystatus]" value="1" />
                          {{ entry_yes }}
                          {% endif %} </label>
                        <label class="radio-inline"> {% if not tvcmscustomsetting_configuration.mainmenustickystatus %}
                          <input type="radio" name="tvcmscustomsetting_configuration[mainmenustickystatus]" value="0" checked="checked" />
                          {{ entry_no }}
                          {% else %}
                          <input type="radio" name="tvcmscustomsetting_configuration[mainmenustickystatus]" value="0" />
                          {{ entry_no }}
                          {% endif %} </label>
                        <br>
                          {{ text_theme_main_des }}
                      </div>
                    </div>
                    {% endif %}
