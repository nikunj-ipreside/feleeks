        $('.input-customthemecolor').removeClass('hidden');
    }
});
$('.themebox').change(function() {
    var val = $(".patternselect:checked").val();
    $('.patten').removeClass('hidden');
    $('.pattencolor').removeClass('hidden');
    if (val == 'bgcolor') {
    } else  {
        $('.patten').removeClass('hidden');
        $('.pattencolor').addClass('hidden');
    }
    if (this.value == '1') {
        $('.themebox1').removeClass('hidden');
    }
    else {
        $('.themebox1').addClass('hidden');
    }
});

$('.patternselect').change(function() {
    if (this.value == 'bgcolor') {
        $('.pattencolor').removeClass('hidden');
        $('.patten').addClass('hidden');
    }
    else  {
        $('.pattencolor').addClass('hidden');
        $('.patten').removeClass('hidden');
    }
});

$(".ttvtheme-pattern-image").click(function(){
  var vall = $(this).attr('id');
  $('#idpatternval').val(vall);
  $(".ttvtheme-pattern-image").removeClass('active');
  $(this).addClass('active');
});


